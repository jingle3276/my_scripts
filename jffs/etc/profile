#This file should exsit on /jffs/etc/profile
#etc/profile checks for and sources /jffs/etc/profile, so it should have picked up from there in my first attempt
#This is system wide env variables
export BRIX2807_HOST=192.168.3.208
export MY_SCRIPTS_HOME=/tmp/mnt/data/my_scripts

alias scripts="cd $MY_SCRIPTS_HOME"
alias jffs="cd /jffs/my_scripts"
alias deploy=$MY_SCRIPTS_HOME/jffs_deploy.sh
alias backup_sda1=nohup $MY_SCRIPTS_HOME/backup_sda1_entware_partition.sh &
